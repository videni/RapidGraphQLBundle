{
    "name": "videni/rapid-graphql-bundle",
    "description": "A rapid GraphQL bundle for Symfony4",
    "type": "symfony-bundle",
    "keywords": [ "rest", "rapid CRUD" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vidy Videni",
            "email": "vidy.videni@gmail.com",
            "homepage": "https://github.com/videni"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "oro/config": "^1.0@dev",
        "oro/doctrine-utils": "^1.0@dev",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/yaml": "^4.1",
        "zendframework/zend-stdlib": "^3.2",
        "pintushi/grid-bundle": "^1.0@dev",
        "limenius/liform-bundle": "^0.13.0",
        "overblog/graphql-bundle": "^0.12"
    },
    "repositories": [
        {
            "url": "https://github.com/videni/Config.git",
            "type": "vcs"
        },
        {
            "url": "https://github.com/pintushi/GridBundle.git",
            "type": "vcs"
        },
        {
            "url": "https://github.com/videni/OroDoctrineUtils.git",
            "type": "vcs"
        }
    ],
    "autoload": {
        "psr-4": { "Videni\\Bundle\\RapidGraphQLBundle\\": "" }
    },
     "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
