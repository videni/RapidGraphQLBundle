{
    "name": "videni/simple-rest-bundle",
    "description": "A rapid RESTfull API bundle for Symfony4",
    "type": "symfony-bundle",
    "keywords": [ "rest", "rapid CRUD" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vidy Videni",
            "email": "vidy.videni@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "oro/config": "^1.0@dev",
        "oro/doctrine-utils": "^1.0@dev",
        "pagerfanta/pagerfanta": "^2.0",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/yaml": "^4.1",
        "willdurand/hateoas-bundle": "^1.4",
        "willdurand/negotiation": "^2.3"
    },
    "repositories": [
        {
            "url": "https://github.com/videni/ChainProcessor.git",
            "type": "vcs"
        },
        {
            "url": "https://github.com/videni/Config.git",
            "type": "vcs"
        },
        {
            "url": "https://github.com/videni/OroDoctrineUtils.git",
            "type": "vcs"
        }
    ],
    "autoload": {
        "psr-4": { "Videni\\Bundle\\RestBundle\\": "" }
    },
     "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
